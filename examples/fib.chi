import std/time { millis }
import std/io { println }

val fib = fn(n: int): int {
    if (n == 0) { 0 }
    else if (n == 1) { 1 }
    else { fib(n - 1) + fib(n - 2) }
}

fib(30)
fib(30)
fib(30)
fib(30)
fib(30)
fib(30)
fib(30)
fib(30)

val start = millis()
fib(30)
val duration = (millis() - start)

println("Took: " + duration as string)
